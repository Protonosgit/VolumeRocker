<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/back"
		android:layout_width="320dp"
		android:layout_height="wrap_content"
		android:layout_marginTop="100dp"
		android:padding="8dp"
		android:background="#252525"
		android:orientation="vertical"
		android:layout_gravity="center_horizontal|center_vertical">
		<ImageView
			android:id="@+id/img2"
			android:layout_width="30dp"
			android:layout_height="30dp"
			android:src="@drawable/ic_cross_w"
			android:scaleType="fitCenter"
			android:layout_gravity="right" />
		<ImageView
			android:id="@+id/img1"
			android:layout_width="80dp"
			android:layout_height="80dp"
			android:src="@drawable/logo_t"
			android:scaleType="fitCenter"
			android:layout_gravity="center_horizontal|center_vertical"
			android:layout_weight="1" />
		<TextView
			android:id="@+id/te2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Volume Rocker allows you to remotely control the media volume of any android phone (requires this app).\n\nCreators: Protonos,Schniekii,Jonathan Straucher\nLibs: wsmanager,KotlinJVM,Okio,ProtractorView"
			android:textSize="16sp"
			android:textColor="#FFFFFF"
			android:layout_weight="1" />
		<TextView
			android:id="@+id/te3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="V 2.4   Powered by BlackNeutron"
			android:textSize="12sp"
			android:textStyle="italic"
			android:textColor="#F06292" />
	</LinearLayout>
</LinearLayout>
